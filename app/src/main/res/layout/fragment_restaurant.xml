<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup257"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@drawable/rectangle_bg_white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <ImageView
            android:id="@+id/imgAvatar"
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:scaleType="fitXY"
            app:layout_constraintHeight_percent="0.26"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/imgBackgroungRadius"
            android:src="@drawable/img_rectangle37"/>
        <ImageView android:id="@+id/imgOverlay"
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:scaleType="fitXY"
            app:layout_constraintHeight_percent="0.26"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/img_rectangle39"/>
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/tbToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            app:layout_constraintEnd_toEndOf="@+id/imgAvatar"
            app:layout_constraintStart_toStartOf="@+id/imgAvatar"
            app:layout_constraintTop_toTopOf="@+id/imgAvatar">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_height="match_parent"
                android:layout_width="match_parent">
                <ImageView
                    android:id="@+id/imageRoundBackButt"
                    android:layout_height="28dp"
                    android:layout_width="28dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_roundbackbutt"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageRoundFavorite"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView android:id="@+id/imageRoundFavorite"
                    android:layout_height="28dp"
                    android:layout_marginStart="271dp"
                    android:layout_width="28dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_roundfavorite"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageRoundBackButt" app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageView
            android:id="@+id/imgBackgroungRadius"
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:scaleType="fitXY"
            android:layout_marginBottom="10dp"
            android:src="@drawable/img_rectangle40"
            app:layout_constraintTop_toBottomOf="@id/imgAvatar"
            app:layout_constraintBottom_toTopOf="@id/svProduct"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.22"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:ellipsize="end"
            android:gravity="start"
            android:id="@+id/tvProductName"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="6dp"
            android:layout_width="0dp"
            android:singleLine="true"
            app:layout_constraintTop_toTopOf="@id/imgBackgroungRadius"
            app:layout_constraintEnd_toEndOf="@+id/imgAvatar"
            app:layout_constraintStart_toStartOf="@+id/imgAvatar"
            android:textSize="24sp"
            android:textColor="@color/bluegray_700"
            android:fontFamily="@font/avenir_next_lt_pro"
            android:textFontWeight="500"
            android:text="Pizzon - Crib Ln"
            tools:ignore="UnusedAttribute" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintTag"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_width="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvProductName"
            app:layout_constraintBottom_toTopOf="@id/constraintInfo">
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnFastFood"
                android:ellipsize="end"
                android:gravity="center"
                android:layout_height="25dp"
                android:layout_width="70dp"
                android:paddingBottom="5dp"
                android:paddingEnd="8dp"
                android:paddingStart="8dp"
                android:paddingTop="5dp"
                android:singleLine="true"
                android:text="@string/lbl_fastfood"
                android:textAllCaps="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/rectangle_bg_gray_202_radius_4"
                android:textSize="12sp"
                android:textColor="@color/bluegray_700"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                tools:ignore="UnusedAttribute" />
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/btnWesternCuisine"
                android:layout_height="25dp"
                android:layout_marginEnd="145dp"
                android:layout_marginStart="8dp"
                android:layout_width="104dp"
                android:paddingBottom="2dp"
                android:paddingEnd="8dp"
                android:paddingStart="8dp"
                android:paddingTop="5dp"
                android:singleLine="true"
                android:textAllCaps="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/btnFastFood"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/rectangle_bg_gray_202_radius_4"
                android:textSize="12sp"
                android:textColor="@color/bluegray_700"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="Western cuisine"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintInfo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:layout_width="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintTag">
            <ImageView android:id="@+id/imageMdiaccesstime"
                android:layout_height="14dp"
                android:layout_width="14dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/txt5Mins"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/txt5Mins"
                android:ellipsize="end"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                app:layout_constraintEnd_toStartOf="@+id/viewEllipse16"
                app:layout_constraintHorizontal_bias="0.02"
                app:layout_constraintStart_toEndOf="@+id/imageMdiaccesstime"
                app:layout_constraintTop_toTopOf="@+id/imageMdiaccesstime"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="@string/lbl_15_mins"/>
            <View
                android:background="@drawable/rectangle_bg_bluegray_700_radius_2"
                android:id="@+id/viewEllipse16"
                android:layout_height="4dp"
                android:layout_marginBottom="4dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="5dp"
                android:layout_width="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageMdiaccesstime"
                app:layout_constraintEnd_toStartOf="@+id/txt11Km"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/txt5Mins"
                app:layout_constraintTop_toTopOf="@+id/txt5Mins"/>
            <TextView android:ellipsize="end" android:id="@+id/txt11Km"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                app:layout_constraintEnd_toStartOf="@+id/imageStar1"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/viewEllipse16"
                app:layout_constraintTop_toTopOf="@+id/imageMdiaccesstime"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="@string/lbl_1_1_km"
                tools:ignore="UnusedAttribute" />
            <ImageView android:id="@+id/imageStar1"
                android:layout_height="14dp"
                android:layout_width="14dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_star"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/txt50"
                app:layout_constraintStart_toEndOf="@+id/txt11Km"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView android:id="@+id/txt50"
                android:ellipsize="end"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                app:layout_constraintEnd_toStartOf="@+id/txt200Rated"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/imageStar1"
                app:layout_constraintTop_toTopOf="@+id/imageMdiaccesstime"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="@string/lbl_5_0"
                tools:ignore="UnusedAttribute" />
            <TextView android:ellipsize="end"
                android:id="@+id/txt200Rated"
                android:layout_height="wrap_content"
                android:layout_marginEnd="79dp"
                android:layout_marginStart="12dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.07"
                app:layout_constraintStart_toEndOf="@+id/txt50"
                app:layout_constraintTop_toTopOf="@+id/imageMdiaccesstime"
                android:textColor="@color/gray_500"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="(200+ rated)"
                tools:ignore="UnusedAttribute" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View android:background="@color/bluegray_50"
            android:id="@+id/viewDivider4"
            android:layout_height="1dp"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="17dp"
            android:layout_width="327dp"
            app:layout_constraintEnd_toEndOf="@+id/imgOverlay"
            app:layout_constraintStart_toStartOf="@+id/imgOverlay"
            app:layout_constraintTop_toBottomOf="@+id/constraintInfo"
            app:layout_constraintBottom_toTopOf="@id/constraintDiscount"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintDiscount"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="15dp"
            android:layout_width="0dp"
            app:layout_constraintEnd_toEndOf="@+id/imgOverlay"
            app:layout_constraintStart_toStartOf="@+id/imgOverlay"
            app:layout_constraintTop_toBottomOf="@+id/viewDivider4">
            <TextView android:ellipsize="end"
                android:gravity="start"
                android:id="@+id/tvDiscount"
                android:layout_height="wrap_content"
                android:layout_marginEnd="182dp"
                android:layout_marginStart="5dp"
                android:layout_width="0dp"
                android:singleLine="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageDiscount"
                app:layout_constraintBaseline_toTopOf="parent"
                android:textSize="14sp"
                android:textFontWeight="500"
                android:textColor="@color/bluegray_700"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:text="Discount 40% pizza"/>
            <ImageView android:id="@+id/imageDiscount"
                android:layout_height="18dp"
                android:layout_width="18dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_discount"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tvDiscount"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/svProduct"
            android:layout_height="0dp"
            android:layout_marginTop="24dp"
            android:layout_width="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgBackgroungRadius">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup228"
                android:layout_height="wrap_content"
                android:layout_width="match_parent">
                <View
                    android:id="@+id/viewDivider1"
                    android:background="@color/bluegray_51"
                    android:layout_height="5dp"
                    android:layout_width="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/tvNewProduct"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                    android:id="@+id/tvNewProduct"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    app:layout_constraintTop_toBottomOf="@id/viewDivider1"
                    app:layout_constraintBottom_toTopOf="@+id/newProductRecyclerView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textSize="20sp"
                    android:textFontWeight="500"
                    android:textColor="@color/bluegray_500"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:text="New Product - Autumn"
                    tools:ignore="HardcodedText,UnusedAttribute" />
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/newProductRecyclerView"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_width="0dp"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintBottom_toTopOf="@+id/tvSpecialDelivery"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvNewProduct"
                    tools:itemCount="2"
                    tools:listitem="@layout/item_product"/>
                <View
                    android:id="@+id/viewDivider2"
                    android:background="@color/bluegray_51"
                    android:layout_height="5dp"
                    android:layout_width="0dp"
                    app:layout_constraintTop_toBottomOf="@+id/newProductRecyclerView"
                    app:layout_constraintBottom_toTopOf="@id/tvSpecialDelivery"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/tvSpecialDelivery"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="24dp"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    app:layout_constraintTop_toBottomOf="@+id/viewDivider2"
                    app:layout_constraintBottom_toTopOf="@+id/specialDeliveryRecyclerView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textSize="20sp"
                    android:textFontWeight="500"
                    android:textColor="@color/bluegray_500"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:text="Special Delivery"
                    tools:ignore="HardcodedText,UnusedAttribute" />
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/specialDeliveryRecyclerView"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_width="0dp"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvSpecialDelivery"
                    tools:itemCount="2"
                    tools:listitem="@layout/item_product"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
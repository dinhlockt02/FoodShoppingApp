<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" 
        android:background="@drawable/rectangle_bg_white_a700_radius_12"
        android:elevation="6dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginVertical="8dp">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn20"
            android:layout_width="56dp"
            android:layout_height="25dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/rectangle_bg_green_500_radius_tl_8_br_8"
            android:ellipsize="end"
            android:fontFamily="@font/avenir_next_lt_pro"
            android:gravity="center"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="13dp"
            android:paddingBottom="3dp"
            android:singleLine="true"
            android:text="20%"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textFontWeight="500"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/cardImageAvatar"
            app:layout_constraintHorizontal_bias="0.07"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardImageAvatar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/constraintTimeSpaceRa"
            app:layout_constraintDimensionRatio="1.0:1.0"
            app:layout_constraintEnd_toStartOf="@+id/txtSteakieazyCa"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="@+id/btn20"
            app:layout_constraintTop_toTopOf="@+id/btn20"
            app:cardCornerRadius="12dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:translationZ="-1dp">
            <ImageView android:id="@+id/imageRectangle15"
                android:layout_height="80dp"
                android:layout_width="80dp"
                android:scaleType="centerCrop"
                android:src="@drawable/img_rectangle15_10"/>
        </androidx.cardview.widget.CardView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintTimeSpaceRa"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginEnd="52dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="22dp"
            android:layout_width="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toEndOf="@+id/cardImageAvatar"
            app:layout_constraintTop_toBottomOf="@+id/txt31CanterburyR">
            <TextView
                android:ellipsize="end"
                android:id="@+id/txt50"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_5_0"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.22"
                app:layout_constraintStart_toEndOf="@+id/imageStar1"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"/>
            <ImageView android:id="@+id/imageStar1"
                android:layout_height="0dp"
                android:layout_marginStart="26dp"
                android:layout_width="14dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_star"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1.0:1.0"
                app:layout_constraintEnd_toStartOf="@+id/txt50"
                app:layout_constraintHorizontal_bias="0.41"
                app:layout_constraintStart_toEndOf="@+id/txt4Km"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txt15Mins"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_15_mins"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/viewEllipse16"
                app:layout_constraintHorizontal_bias="0.03"
                app:layout_constraintStart_toEndOf="@+id/imageMdiaccesstime"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"/>
            <TextView android:ellipsize="end" android:id="@+id/txt4Km"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_3_km"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageStar1"
                app:layout_constraintHorizontal_bias="0.08"
                app:layout_constraintStart_toEndOf="@+id/viewEllipse16"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@color/bluegray_700"
                android:textSize="12sp"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"/>
            <ImageView android:id="@+id/imageMdiaccesstime"
                android:layout_height="0dp"
                android:layout_width="14dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintDimensionRatio="1.0:1.0"
                app:layout_constraintEnd_toStartOf="@+id/txt15Mins"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <View
                android:background="@drawable/rectangle_bg_bluegray_700_radius_2"
                android:id="@+id/viewEllipse16"
                android:layout_height="4dp"
                android:layout_marginBottom="5dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="5dp"
                android:layout_width="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageMdiaccesstime"
                app:layout_constraintEnd_toStartOf="@+id/txt4Km"
                app:layout_constraintHorizontal_bias="0.01"
                app:layout_constraintStart_toEndOf="@+id/txt15Mins"
                app:layout_constraintTop_toTopOf="@+id/imageMdiaccesstime"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView android:ellipsize="end"
            android:gravity="start"
            android:id="@+id/txtSteakieazyCa"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="16dp"
            android:layout_width="0dp" android:singleLine="true"
            android:text="Savagan - Mounthoolie abchbhuhwu"
            app:layout_constraintBottom_toTopOf="@+id/txt31CanterburyR"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardImageAvatar"
            app:layout_constraintTop_toTopOf="@+id/btn20"
            android:textSize="16sp"
            android:textColor="@color/bluegray_700"
            android:fontFamily="@font/avenir_next_lt_pro"
            android:textFontWeight="500"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txt31CanterburyR"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="4dp"
            android:layout_width="0dp" android:singleLine="true"
            android:text="74 Mounthoolie Lane, Sundridge, Đăk Lăk, Việt Nam"
            android:textColor="@color/gray_500"
            android:textSize="12sp"
            android:fontFamily="@font/avenir_next_lt_pro"
            android:textFontWeight="500"
            app:layout_constraintBottom_toTopOf="@+id/constraintTimeSpaceRa"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardImageAvatar"
            app:layout_constraintTop_toBottomOf="@+id/txtSteakieazyCa"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
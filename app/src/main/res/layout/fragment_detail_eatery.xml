<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <data>
        <variable
            name="viewModel"
            type="xyz.daijoubuteam.foodshoppingapp.client.home.detail_eatery.DetailEateryViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup257"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@drawable/rectangle_bg_white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/tbToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:translationZ="10dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_height="match_parent"
                android:layout_width="match_parent">
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floating_action_button_home"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/back_home_button"
                    app:backgroundTint="@android:color/transparent"
                    app:borderWidth="0dp"
                    app:elevation="0dp"
                    app:pressedTranslationZ="0dp"
                    app:srcCompat="@drawable/img_roundbackbutt"/>
                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floating_action_save"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/back_home_button"
                    app:backgroundTint="@android:color/transparent"
                    app:borderWidth="0dp"
                    app:elevation="0dp"
                    app:pressedTranslationZ="0dp"
                    app:srcCompat="@drawable/img_roundfavorite"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageView
            android:id="@+id/imgThumbnailEatery"
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:scaleType="fitXY"
            app:layout_constraintHeight_percent="0.3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/constraintBackgroundRadius"
            android:src="@drawable/img_rectangle37"
            android:contentDescription="@string/thumbnail_eatery" />
        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floating_action_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@id/constraintBackgroundRadius"
            app:layout_constraintEnd_toEndOf="@id/constraintGroup257"
            app:layout_constraintTop_toBottomOf="@id/imgThumbnailEatery"
            app:fabSize="mini"
            app:backgroundTint="@color/white_A700"
            app:srcCompat="@drawable/ic_outline_info_24"
            android:contentDescription="@string/button_information_eatery" />
        <View
            android:id="@+id/tempView"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toBottomOf="@id/imgThumbnailEatery"
            app:layout_constraintBottom_toBottomOf="@id/imgThumbnailEatery"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintBackgroundRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/rectangle_bg_white_radius_t_20"
            android:paddingBottom="20dp"
            app:layout_constraintTop_toTopOf="@id/tempView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.24"
            app:layout_constraintStart_toStartOf="parent">
            <TextView
                android:id="@+id/tvProductName"
                android:ellipsize="end"
                android:gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="10dp"
                android:layout_width="0dp"
                android:singleLine="true"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:textSize="24sp"
                android:textColor="@color/bluegray_700"
                android:fontFamily="@font/avenir_next_lt_pro"
                android:textFontWeight="500"
                android:text="@{viewModel.selectedProperty.name}"
                tools:ignore="HardcodedText,UnusedAttribute" />
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintTag"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_width="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvProductName"
                app:layout_constraintBottom_toTopOf="@id/constraintInfo">
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnFastFood"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:layout_height="25dp"
                    android:layout_width="70dp"
                    android:paddingVertical="5dp"
                    android:paddingHorizontal="8dp"
                    android:layout_margin="4dp"
                    android:singleLine="true"
                    android:text="@string/lbl_fastfood"
                    android:textAllCaps="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@drawable/rectangle_bg_gray_202_radius_4"
                    android:textSize="12sp"
                    android:textColor="@color/bluegray_700"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    tools:ignore="UnusedAttribute" />
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnWesternCuisine"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:layout_height="25dp"
                    android:layout_marginEnd="145dp"
                    android:layout_marginStart="8dp"
                    android:layout_width="104dp"
                    android:paddingVertical="5dp"
                    android:paddingHorizontal="8dp"
                    android:layout_margin="4dp"
                    android:singleLine="true"
                    android:textAllCaps="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toEndOf="@+id/btnFastFood"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@drawable/rectangle_bg_gray_202_radius_4"
                    android:textSize="12sp"
                    android:textColor="@color/bluegray_700"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    android:text="Western cuisine"
                    tools:ignore="HardcodedText,UnusedAttribute" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintInfo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="12dp"
                android:layout_width="0dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintTag">
                <ImageView android:id="@+id/imageMdiaccesstime"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_marginStart="0dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    app:layout_constraintBottom_toBottomOf="@id/txt5Mins"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/txt5Mins"/>
                <TextView
                    android:id="@+id/txt5Mins"
                    android:ellipsize="end"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    app:layout_constraintStart_toEndOf="@id/imageMdiaccesstime"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:textColor="@color/bluegray_700"
                    android:textSize="12sp"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    android:text="@string/lbl_15_mins"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_700_radius_2"
                    android:id="@+id/viewEllipse16"
                    android:layout_height="4dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_width="4dp"
                    app:layout_constraintBottom_toBottomOf="@+id/txt5Mins"
                    app:layout_constraintEnd_toStartOf="@+id/txt11Km"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toEndOf="@+id/txt5Mins"
                    app:layout_constraintTop_toTopOf="@+id/txt5Mins"/>
                <TextView android:ellipsize="end" android:id="@+id/txt11Km"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="12dp"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    app:layout_constraintEnd_toStartOf="@+id/imageStar1"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toEndOf="@+id/viewEllipse16"
                    app:layout_constraintTop_toTopOf="@+id/txt5Mins"
                    app:layout_constraintBottom_toBottomOf="@id/txt5Mins"
                    android:textColor="@color/bluegray_700"
                    android:textSize="12sp"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    android:text="@string/lbl_1_1_km"
                    tools:ignore="UnusedAttribute" />
                <ImageView android:id="@+id/imageStar1"
                    android:layout_height="14dp"
                    android:layout_width="14dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    app:layout_constraintBottom_toBottomOf="@id/txt5Mins"
                    app:layout_constraintEnd_toStartOf="@+id/txt50"
                    app:layout_constraintStart_toEndOf="@+id/txt11Km"
                    app:layout_constraintTop_toTopOf="@id/txt5Mins"/>
                <TextView android:id="@+id/txt50"
                    android:ellipsize="end"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    app:layout_constraintEnd_toStartOf="@+id/txt200Rated"
                    app:layout_constraintHorizontal_bias="0.03"
                    app:layout_constraintStart_toEndOf="@+id/imageStar1"
                    app:layout_constraintTop_toTopOf="@+id/txt5Mins"
                    app:layout_constraintBottom_toBottomOf="@id/txt5Mins"
                    android:textColor="@color/bluegray_700"
                    android:textSize="12sp"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    android:text="@{Double.toString(viewModel.selectedProperty.average_rating_count)}"
                    tools:ignore="UnusedAttribute" />
                <TextView android:ellipsize="end"
                    android:id="@+id/txt200Rated"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="81dp"
                    android:layout_marginStart="6dp"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.07"
                    app:layout_constraintStart_toEndOf="@+id/txt50"
                    app:layout_constraintTop_toTopOf="@+id/txt5Mins"
                    app:layout_constraintBottom_toBottomOf="@id/txt5Mins"
                    android:textColor="@color/gray_500"
                    android:textSize="12sp"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:textFontWeight="500"
                    android:text="(200+ rated)"
                    tools:ignore="UnusedAttribute" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View android:background="@color/bluegray_50"
                android:id="@+id/viewDivider4"
                android:layout_height="1dp"
                android:layout_width="0dp"
                android:layout_marginTop="17dp"
                app:layout_constraintEnd_toEndOf="@+id/constraintInfo"
                app:layout_constraintStart_toStartOf="@+id/constraintInfo"
                app:layout_constraintTop_toBottomOf="@+id/constraintInfo"
                app:layout_constraintBottom_toTopOf="@id/constraintDiscount"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintDiscount"
                android:layout_height="wrap_content"
                android:layout_marginEnd="24dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_width="0dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewDivider4">
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/tvDiscount"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="24dp"
                    android:layout_marginStart="5dp"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageDiscount"
                    app:layout_constraintBaseline_toTopOf="parent"
                    android:textSize="14sp"
                    android:textFontWeight="500"
                    android:textColor="@color/bluegray_700"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:text="Discount 40% pizza"/>
                <ImageView android:id="@+id/imageDiscount"
                    android:layout_height="18dp"
                    android:layout_width="18dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_discount"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tvDiscount"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.core.widget.NestedScrollView
            android:id="@+id/svProduct"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:clipToPadding="false"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintBackgroundRadius">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <View
                    android:id="@+id/viewDivider1"
                    android:layout_width="0dp"
                    android:layout_height="5dp"
                    android:background="@color/bluegray_51"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tvNewProduct"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:gravity="start"
                    android:singleLine="true"
                    android:text="For you"
                    android:textColor="@color/bluegray_500"
                    android:textFontWeight="500"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toTopOf="@+id/forYouProductRecyclerView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/viewDivider1"
                    tools:ignore="HardcodedText,UnusedAttribute" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/forYouProductRecyclerView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintBottom_toTopOf="@+id/tvSpecialDelivery"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvNewProduct"
                    tools:itemCount="2"
                    tools:listitem="@layout/item_product" />
                <View
                    android:id="@+id/viewDivider2"
                    android:layout_width="0dp"
                    android:layout_height="5dp"
                    android:background="@color/bluegray_51"
                    app:layout_constraintBottom_toTopOf="@id/tvSpecialDelivery"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/forYouProductRecyclerView" />

                <TextView
                    android:id="@+id/tvSpecialDelivery"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="24dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/avenir_next_lt_pro"
                    android:gravity="start"
                    android:singleLine="true"
                    android:text="Special Delivery"
                    android:textColor="@color/bluegray_500"
                    android:textFontWeight="500"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toTopOf="@+id/specialDeliveryRecyclerView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/viewDivider2"
                    tools:ignore="HardcodedText,UnusedAttribute" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/specialDeliveryRecyclerView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvSpecialDelivery"
                    tools:itemCount="2"
                    tools:listitem="@layout/item_product" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>